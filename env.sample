# Azure OpenAI Configuration
AZURE_API_KEY=your_azure_openai_api_key_here
AZURE_API_BASE=https://your-resource-name.openai.azure.com/
AZURE_DEPLOYMENT=your-deployment-name

# Database Configuration
# PostgreSQL (for local development with docker-compose-db.yml)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=mcp_db
POSTGRES_USER=mcp_user
POSTGRES_PASSWORD=secret

# Neo4j (for local development with docker-compose-db.yml)
# Note: When using docker-compose.yml (full stack), use bolt://neo4j:7687
NEO4J_URI=bolt://localhost:7688
NEO4J_USER=neo4j
NEO4J_PASSWORD=test12345

# Application Configuration
MCP_SERVER_URL=http://localhost:8000 